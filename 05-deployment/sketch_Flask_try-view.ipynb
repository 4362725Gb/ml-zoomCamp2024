    "# ping pong test\n",
    "\n",
    "from flask import Flask\n",
    " \n",
    "app = Flask('ping')\n",
    " \n",
    "@app.route('/ping', methods=['GET'])\n",
    "def ping():\n",
    "    return \"PONG\"\n",
    " \n",
    "if __name__ == \"__main__\":\n",
    "    app.run(debug=True, host='0.0.0.0', port=9696)"
   ]
